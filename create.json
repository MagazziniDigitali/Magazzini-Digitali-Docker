{
        "AttachStdin": false,
        "AttachStdout": false,
        "AttachStderr": false,
        "Tty": false,
        "OpenStdin": false,
        "StdinOnce": false,
        "Image": "local/ffviewer:latest",
        "NetworkDisabled": false,
        "ExposedPorts": {
		"5900/tcp": {}
        },
	"Cmd": ["/usr/bin/x11vnc", "-input", "MBK,MBK", "-nopw", "-create", "-gone", "touch /root/left.txt", "-env", "FD_PROG=/usr/bin/viewer.sh http://www.bncf.firenze.sbn.it/"],
        "HostConfig": {
	  "Memory": 268435456,
	  "MemorySwap": -1,
	  "CpuShares": 512,
          "PortBindings": { "5900/tcp": [{ "HostPort": "5900" }] },
          "PublishAllPorts": false,
          "Privileged": false,
          "ExtraHosts": null,
          "CapAdd": ["NET_ADMIN"],
          "CapDrop": ["MKNOD"],
          "NetworkMode": "bridge"
        }
}
